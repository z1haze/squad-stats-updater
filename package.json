    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "babel-jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
